                                                                aria-valuemax="100"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div><i class="material-icons icon-muted icon-40pt ml-3">contacts</i>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="card">
                                    <div class="card-header card-header-large bg-white">
                                        <h4 class="card-header__title">All jobs</h4>
                                    </div>
                                    <div class="card-header">
                                        <form class="form-inline" method="post" action="/jobs">
                                            <label class="mr-sm-2" for="inlineFormFilterBy">Add question:</label>
                                            <input type="text" class="form-control mb-2 mr-sm-2 mb-sm-0"
                                                id="inlineFormFilterBy" name="title" placeholder="Type  question done">

                                            <label class="mr-sm-2" for="inlineFormFilterBy">Platform:</label>
                                            <input type="text" class="form-control mb-2 mr-sm-2 mb-sm-0"
                                                id="inlineFormFilterBy" name="platform" placeholder="Cource hero">

                                            <label class="mr-sm-2" for="inlineFormFilterBy">Add amount to be paid in
                                                ksh:</label>
                                            <input type="number" class="form-control mb-2 mr-sm-2 mb-sm-0"
                                                id="inlineFormFilterBy" name="amount" placeholder="20">

                                            <div class="form-group">
                                                <button class="btn btn-block btn-primary" type="submit">add</button>
                                            </div>
                                        </form>
                                    </div>


                                    <div class="table-responsive border-bottom" data-toggle="lists"
                                        data-lists-values='["js-lists-values-employee-name"]'>

                                        <table class="table mb-0 thead-border-top-0">
                                            <thead>
                                                <tr>



                                                    <th>Question</th>
                                                    <th style="width: 150px;">Platform</th>
                                                    <th style="width: 37px;">Status</th>
                                                    <th style="width: 120px;">Date</th>
                                                    <th style="width: 51px;">Earnings</th>

                                                </tr>
                                            </thead>
                                            <tbody class="list" id="staff">
                                                <% if (totaljobs==0) { %>
                                                    <p>No jobs done</p>
                                                    <% }else{%>
                                                        <% jobs.forEach (job=>{%>
                                                            <tr class="selected">


                                                                <td>

                                                                    <div class="media align-items-center">
                                                                        <div class="media-body">

                                                                            <span class="js-lists-values-employee-name">
                                                                                <%=job.jobTitle%>
                                                                            </span>

                                                                        </div>
                                                                    </div>

                                                                </td>

                                                                <td>
                                                                    <div class="media align-items-center">
                                                                        <a href="#">
                                                                            <%= job.platform%>
                                                                        </a>
                                                                    </div>
                                                                </td>


                                                                <td><span class="badge 
                                                                    <% if(job.status == 'pending'){ %>
                                                                        badge-warning
                                                                        <% }else if(job.status == 'rejected'){ %>
                                                                        badge-danger
                                                                        <%}else{%>
                                                                            badge-success
                                                                            <%}%>
                                                                ">
                                                                        <%= job.status%>
                                                                    </span></td>
                                                                <td><small class="text-muted">
                                                                        <%= job.createdAt%>
                                                                    </small></td>
                                                                <td>
                                                                    <%= job.amount %>
                                                                </td>
                                                            </tr>
                                                            <%})%>
                                                                <%}%>


                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <!-- END FIRST TAB CONTENT -->
                            </div>

                        </div>
                    </div>


                </div>
                <!-- // END header-layout__content -->

            </div>
            <!-- // END header-layout -->

        </div>
        <!-- // END drawer-layout__content -->

